-- Cargar Rayfield
local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

-- Ventana principal
local Window = Rayfield:CreateWindow({
    Name = "Scripts Made by zeroz560",
    LoadingTitle = "Cargando Hub...",
    LoadingSubtitle = "by zeroz560",
    ConfigurationSaving = {
        Enabled = true,
        FolderName = "Zeroz560Hub",
        FileName = "Config"
    },
    KeySystem = false
})

-- PestaÃ±a Scripts (Ro-bio y especÃ­ficos)
local ScriptsTab = Window:CreateTab("Scripts", 4483362458)

local player = game.Players.LocalPlayer

-- === RO-BIO INJECTION EXPERIMENT ===
ScriptsTab:CreateButton({
    Name = "Ro-bio injection experiment",
    Callback = function()
        Rayfield:Notify({Title = "Cargando Ro-bio...", Content = "ActivÃ¡ndose ðŸ˜ˆ", Duration = 5})

        spawn(function()
            local RayfieldInner = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

            local WindowInner = RayfieldInner:CreateWindow({
                Name = "Ro-bio injection experiment Tools",
                LoadingTitle = "Ro-bio Tools",
                LoadingSubtitle = "by zeroz560",
                ConfigurationSaving = {Enabled = true, FolderName = "RobioTools", FileName = "Config"},
                KeySystem = false
            })

            local PrincipalTab = WindowInner:CreateTab("Principal", 4483362458)
            local CreditosTab = WindowInner:CreateTab("Credits Important", 4483362458)
            local VolverTab = WindowInner:CreateTab("Volver script principal", 4483362458)

            local player = game.Players.LocalPlayer
            local playerName = player.Name
            local playerUserId = player.UserId

            local function findPlayerLab()
                local labs = workspace.Labs
                local attempts = 0
                local maxAttempts = 60
                local debugValues = ""

                while attempts < maxAttempts do
                    for i = 1, 12 do
                        local labName = tostring(i)
                        local lab = labs:FindFirstChild(labName)
                        if lab then
                            local owner = lab:FindFirstChild("Owner")
                            if owner then
                                local value = owner.Value
                                debugValues = debugValues .. "Lab " .. labName .. ": " .. tostring(value) .. "\n"
                                if (owner:IsA("StringValue") and (value == playerName or string.find(tostring(value), playerName))) 
                                    or (owner:IsA("IntValue") and value == playerUserId) 
                                    or (owner:IsA("ObjectValue") and value == player) then
                                    RayfieldInner:Notify({Title = "Lab detectada!", Content = "Usando lab " .. labName})
                                    return lab, labName
                                end
                            else
                                debugValues = debugValues .. "Lab " .. labName .. ": No Owner\n"
                            end
                        end
                    end
                    attempts = attempts + 1
                    wait(1)
                end

                RayfieldInner:Notify({Title = "No lab encontrada", Content = "Values:\n" .. debugValues})
                return nil
            end

            local function autoVirus()
                local myLab, labNumber = findPlayerLab()
                if not myLab then return end
                local character = player.Character or player.CharacterAdded:Wait()
                local hrp = character:WaitForChild("HumanoidRootPart")
                local firstPart = workspace.Displays.PassDisplay2:GetChildren()[10]
                if firstPart and firstPart:IsA("BasePart") then hrp.CFrame = firstPart.CFrame + Vector3.new(0,5,0)
                else RayfieldInner:Notify({Title="Error",Content="PassDisplay2 no encontrado"}) return end
                wait(10)
                local controlPart = myLab["Control Panel"].Build.MeshPart
                if controlPart then hrp.CFrame = controlPart.CFrame + Vector3.new(0,5,0)
                else RayfieldInner:Notify({Title="Error",Content="Control Panel no encontrado"}) return end
                local spawnButton = myLab["Control Panel"].Buttons.Spawn
                if spawnButton then game:GetService("ReplicatedStorage").RemoteEvents.ButtonClick:FireServer(spawnButton)
                else RayfieldInner:Notify({Title="Error",Content="BotÃ³n Spawn no encontrado"}) return end
                wait(0.7)
                local finalPart = myLab["Cylinder.079_Cylinder.055"]
                if finalPart then hrp.CFrame = finalPart.CFrame + Vector3.new(0,5,0) end
                RayfieldInner:Notify({Title="Ã‰xito",Content="Auto Virus completado en lab "..labNumber})
            end

            local function autoKill()
                local myLab = findPlayerLab()
                if myLab then game:GetService("ReplicatedStorage").RemoteEvents.ButtonClick:FireServer(myLab["Control Panel"].Buttons.Electrocute) end
            end

            local function aguantaVirus()
                local myLab = findPlayerLab()
                if myLab then game:GetService("ReplicatedStorage").RemoteEvents.ButtonClick:FireServer(myLab["Control Panel"].Buttons.Hold) end
            end

            PrincipalTab:CreateButton({Name="Auto virus",Callback=function() spawn(autoVirus) end})
            PrincipalTab:CreateButton({Name="Auto kill",Callback=autoKill})
            PrincipalTab:CreateButton({Name="Aguanta Virus",Callback=aguantaVirus})

            CreditosTab:CreateParagraph({Title="Script made by",Content="zeroz560"})
            CreditosTab:CreateParagraph({Title="Youtube",Content="zeroz560"})
            CreditosTab:CreateParagraph({Title="TikTok",Content="zeroz560"})
            CreditosTab:CreateButton({Name="Cerrar GUI",Callback=function() RayfieldInner:Destroy() end})

            VolverTab:CreateButton({
                Name = "Volver al menÃº principal",
                Callback = function()
                    RayfieldInner:Destroy()
                    Rayfield:Notify({Title = "Â¡Volviste!", Content = "EstÃ¡s en el hub principal ðŸ”¥", Duration = 6})
                end
            })

            RayfieldInner:Notify({Title="Listo",Content="Ro-bio cargado ðŸ˜"})
        end)
    end
})

